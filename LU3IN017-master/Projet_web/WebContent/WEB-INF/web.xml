<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd"> 
<web-app> 
	<display-name>Projet_web</display-name> 
	<servlet> 
		<servlet-name>Authentification</servlet-name> 
		<servlet-class>servlets.Authentification_servlets</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>Friends</servlet-name> 
		<servlet-class>servlets.Friends_servlets</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>Message</servlet-name> 
		<servlet-class>servlets.Message_servlets</servlet-class> 
	</servlet>
	<servlet> 
		<servlet-name>User</servlet-name> 
		<servlet-class>servlets.User_servlets</servlet-class> 
	</servlet> 
	
	<servlet-mapping> 
		<servlet-name>Authentification</servlet-name> 
		<url-pattern>/authentification</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>Friends</servlet-name> 
		<url-pattern>/friends</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>Message</servlet-name> 
		<url-pattern>/message</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>User</servlet-name> 
		<url-pattern>/user</url-pattern> 
	</servlet-mapping>
</web-app>